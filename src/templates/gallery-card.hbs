{{#each this}}
    <a class="photo-link" href="{{largeImageURL}}">
        <div class="photo-card">
            <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" class="img-item" />
            <div class="info">
                <p class="info-item">
                    <b>Likes:</b>{{likes}}
                </p>
                <p class="info-item">
                    <b>Views:</b>{{views}}
                </p>
                <p class="info-item">
                    <b>Comments:</b>{{comments}}
                </p>
                <p class="info-item">
                    <b>Downloads:</b>{{downloads}}
                </p>
            </div>
        </div>
    </a>
{{/each}}